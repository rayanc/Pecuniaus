<div class="row">
    <div class="col-md-12">
        <div class="block full">
            <div class="block-title">
                <h2>Processors</h2>
            </div>
            <div id="dvEditProcessor" data-source-url="@Url.Action("AddProcessor")">
                @{ Html.RenderAction("AddProcessor");  }
            </div>

            <div class="table-responsive" id="dvProcessorsListing" data-source-url="@Url.Action("ListProcessors")">
                @{ Html.RenderAction("ListProcessors");  }
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</div>
<script type="text/javascript">
    function EditProcessor(processorID) {
     $('#dvEditProcessor').load('@Url.Action("EditProcessor")?id=' + processorID);
    }
</script>