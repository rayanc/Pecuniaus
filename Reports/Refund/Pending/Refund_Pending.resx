  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>/* Refunds Pending*/ Select M.MerchantId as 'Merchant ID', M.BusinessName as 'Merchant Name', C.ContractNumber as 'Contract Number', C.LoanedAmount as 'MCA Amount', C.ownedAmount as 'Total Owned Amount', mr.TotalPaidAmount as 'Total Paid Amount', mr.DatePaindInFull as 'Date Paid In Full', mr.AmountToRefund as 'Amount To Refund', mr.ProcessorID as 'Processor Number', pl.Name as 'Processor Company', pa.ProcessedDate as 'Last Activity Date' from tb_merchants M join tb_contracts C on M.merchantid=C.merchantid and C.statusid=20007 JOIN tb_Merchantrefunds mr on C.contractId=mr.contractid and M.merchantid=mr.merchantid JOIN tb_processoractivities pa on C.ContractId=pa.ContractId and pa.MerchantId=C.MerchantId and pa.ProcessedDate=(Select max(processeddate) from tb_processoractivities where merchantId=pa.merchantId and contractId=pa.contractId) JOIN lkp_tb_processorlist pl on mr.processorID=pl.ProcessorId Where mr.RefundDone=0 group by C.contractId having mr.TotalPaidAmount &gt; C.OwnedAmount;</value>
